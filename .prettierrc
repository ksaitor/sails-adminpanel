{
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "arrowParens": "avoid",
  "printWidth": 130,
  "tabWidth": 2,
  "proseWrap": "always",
  "jsxBracketSameLine": true,
  "jsxSingleQuote": true,
  "noImplicit": [],
  "comma": "none",
  "respectExplicit": ["callParens", "objectBraces"],
  "inlineAssignmentsTo": ["control"]
}
