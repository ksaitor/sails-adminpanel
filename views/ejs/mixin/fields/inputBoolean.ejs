<div class="checkbox relative">
    <% if (value) { %>
        <input type="checkbox" id="form-<%= key %>" name="<%= key %>" value="1" <%= field.config.disabled && 'disabled' || ''%> checked>
    <% } else { %>
        <input type="checkbox" id="form-<%= key %>" name="<%= key %>" value="0" <%= field.config.disabled && 'disabled' || ''%>>
    <% } %>
    <span class="check"></span>
</div>

<script>
    $('.checkbox #form-<%= key -%>').on('click', function(){
        let val = $(this).val()
        if(val === '0'){
            $(this).val('1')
        } else {
            $(this).val('0')
        }
    })
</script>
