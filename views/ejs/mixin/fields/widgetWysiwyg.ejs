<% if(Boolean((adminpanel.configHelper.getConfig()).ckeditor5)){ %>
<div id="vue-app"></div>
<% } %>
<% if(!Boolean((adminpanel.configHelper.getConfig()).ckeditor5)){ %>
<textarea <%= field.config.required && 'required' %> <%= field.config.disabled && 'disabled'%> name="<%= key %>" id="form-<%= key %>"><%=value%></textarea>
<script>
    CKEDITOR.replace('form-<%= key %>');
</script>
<% } %>