<!DOCTYPE html>

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Admin</title>

    <!-- Main CSS -->
    <link rel="stylesheet" href="/admin/assets/build/style/style.min.css" />
    <!-- Main Script -->
    <script src="/admin/assets/build/js/script.min.js"></script>
    <script src="/admin/assets/build/js/ace/ace.js"></script>
    <script src="/admin/assets/build/js/jsoneditor/jsoneditor.min.js"></script>

    <!-- CKEditor -->
    <script src="/admin/assets/build/js/ckeditor/ckeditor.js"></script>    
   
    <!-- Quill Editor -->
    <script src='//cdn.quilljs.com/1.3.6/quill.min.js'></script>
    <link rel='stylesheet' href='//cdn.quilljs.com/1.3.6/quill.snow.css'>

    <!-- Vue -->
    <script src="/admin/assets/build/js/vue/vue-app.js"></script>
</head>

<body cds-text="body" cds-theme="dark" cds-layout="vertical gap:lg p:lg">
    <div class="main-container">
        <header class="header-6">
            <div class="branding">
                <a href="">
                    <i class="las la-rocket logo"></i>
                    <span class="brand-title"><%= adminpanel.menuHelper.getBrandTitle() %></span>
                </a>
            </div>
            <div class="header__inner">
                <!-- Burger -->
                <div class="header__menu">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>


                <div class="header__login">
                    <% if (req.session.UserAP) { %>
                    <% if (req.session.adminPretender && req.session.adminPretender.id) { %>
                    <i class="las la-mask"></i>&nbsp;
                    <% } %>
                    <a class="header__name" href="#">
                        <span><%= req.session.UserAP.fullName %></span>
                        <i class="las la-user"></i>
                    </a>
                    <a class="header__out nav-link" href="/admin/model/userap/logout">
                        <i class="las la-door-open"></i>
                    </a>
                    <% } %>
                </div>
            </div>
        </header>
        <%- include("mixin/sections") %>